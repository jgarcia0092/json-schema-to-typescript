{
  "name": "Convert relative $refs to absolute",
  "in": {
    "id": "foo",
    "type": ["object"],
    "properties": {
      "a": {
        "$ref": "#",
        "id": "a"
      },
      "b": {
        "$ref": "#/properties/b",
        "id": "b"
      }
    },
    "required": []
  },
  "out": {
    "id": "foo",
    "type": "object",
    "properties": {
      "a": {
        "$ref": "<%= filename %>#",
        "id": "a"
      },
      "b": {
        "$ref": "<%= filename %>#/properties/b",
        "id": "b"
      }
    },
    "required": [],
    "additionalProperties": true
  }
}